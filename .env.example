GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.0-flash

# Railway runtime provides this internal URL automatically.
DATABASE_URL=

# Set this for local/Vercel/any non-Railway runtime.
# Example: postgresql://postgres:password@metro.proxy.rlwy.net:5432/railway
DATABASE_PUBLIC_URL=

# Optional: set to false for local non-SSL Postgres.
DATABASE_SSL=true

# Optional tuning
MAX_RESULT_ROWS=50
SCHEMA_CACHE_SECONDS=300
GEMINI_TIMEOUT_SECONDS=20
